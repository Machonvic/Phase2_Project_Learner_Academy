<?xml version = "1.0" encoding = "utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="com.learneracademy">
    <class name="ClassSubject" table="class_subject">
        <id name="ID" column="ID">
            <generator class="increment"/>
        </id>
      <property name = "CLASSID" column = "class_id" type = "integer"/>
      <property name = "SUBJECTID" column = "subject_id" type = "integer"/>
      <property name = "TEACHERID" column = "teacher_id" type = "integer"/>
     

    </class>
</hibernate-mapping>




